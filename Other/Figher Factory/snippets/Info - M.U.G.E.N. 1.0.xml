<?xml version="1.0" encoding="UTF-8"?>
<snippet>

  <field name="name" title="Name" type="string"/>
  <field name="dname" title="Display name" type="string"/>
  <field name="author" title="Author" type="string"/>
  <field name="date" title="Version date" type="string"/>
  <field name="coordx" title="Local coordinates (X)" type="int"/>
  <field name="coordy" title="Local coordinates (Y)" type="int"/>

  <code>
    [Info]
    name = "@name"
    displayname = "@dname"
    versiondate = @date
    mugenversion = 1.0
    author = "@author"
    localcoord = @coordx,@coordy
    pal.defaults = CURSOR
  </code>

</snippet>